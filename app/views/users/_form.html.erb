<%= form_with(model: @user, html: { multipart: true }, local: true) do |f| %>

  <div>
    <%=f.label :image %>
    <%=f.file_field :image %>
  </div>

  <p><%= f.label :full_name %></p>
  <p><%= f.text_field :full_name %></p>

  <p><%= f.label :birth_date %></p>
  <p><%= f.text_field :birth_date%></p>

  <p><%= f.submit class:"btn btn-light" %></p>

<% end %>
